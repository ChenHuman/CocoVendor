<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <PreferenceCategory
        android:title="系统">
        <Preference
            android:title="系统参数设置"/>
    </PreferenceCategory>

    <PreferenceCategory
        android:title="网络">
        <Preference
            android:title="网络设置"/>
        <Preference
            android:title="服务器设置"/>
    </PreferenceCategory>

    <PreferenceCategory
        android:title="安全">
        <Preference
            android:key="key_change_pswd"
            android:title="后台密码修改">
            <intent
                android:targetPackage="cn.edu.stu.max.cocovendor"
                android:targetClass="cn.edu.stu.max.cocovendor.Activity.LoginActivity"/>
        </Preference>
        <Preference
            android:key="key_cam_setting"
            android:title="监控设置">
            <intent
                android:targetPackage="cn.edu.stu.max.cocovendor"
                android:targetClass="cn.edu.stu.max.cocovendor.Activity.CameraSettingActivity"/>
        </Preference>
    </PreferenceCategory>

    <PreferenceCategory
        android:title="关于">
        <Preference
            android:title="本机信息">
            <intent
                android:targetPackage="cn.edu.stu.max.cocovendor"
                android:targetClass="cn.edu.stu.max.cocovendor.Activity.SheetInfoActivity"/>
        </Preference>
    </PreferenceCategory>

</PreferenceScreen>